@model CarFilterViewModel
@{
    ViewData["Title"] = "Home Page";
}



<!-- carousel start -->
<div class="header-carousel">
    <div id="carouselıd" class="carousel slide" data-bs-ride="carousel" data-bs-interval="false">
        <ol class="carousel-indicators">
            <li data-bs-target="#carouselıd" data-bs-slide-to="0" class="active" aria-current="true" aria-label="first slide"></li>
            <li data-bs-target="#carouselıd" data-bs-slide-to="1" aria-label="second slide"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="carousel-item active">
                <img src="~/themes/img/carousel-2.jpg" class="img-fluid w-100" alt="first slide" />
                <div class="carousel-caption">
                    <div class="container py-4">
                        <div class="row g-5">
                            <div class="col-lg-6 fadeınleft animated" data-animation="fadeınleft" data-delay="1s" style="animation-delay: 1s;">
                                <div class="bg-secondary rounded p-5">
                                    <h4 class="text-white mb-4">Araç Kirala</h4>
                                    <form asp-controller="Home" asp-action="Filter" method="post">
                                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                        <div class="row g-3">
                                            <div class="col-12">
                                                <select asp-for="VehicleTypeId" class="form-select" multiple aria-label="default select example">
                                                    <option selected disabled hidden>Araç modelini seç</option>
                                                    @foreach (var item in ViewBag.Models)
                                                    {
                                                        <option value="@item.Id">@item.Name</option>
                                                    }
                                                </select>
                                            </div>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-map-marker-alt"></span> <span class="ms-1">Alış Yeri</span>
                                                    </div>
                                                    <select asp-for="PickupLocationId" class="form-select">
                                                        <option selected disabled hidden>Lokasyon seçin</option>
                                                        @foreach (var location in ViewBag.Locations)
                                                        {
                                                            <option value="@location.Id">@location.Region</option>
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <a href="#" class="text-start text-white d-block mb-2">Farklı bir iade yerine mi ihtiyacınız var?</a>
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-map-marker-alt"></span><span class="ms-1">İade Yeri</span>
                                                    </div>
                                                    <select asp-for="ReturnLocationId" class="form-select">
                                                        <option selected disabled hidden>Lokasyon seçin</option>
                                                        @foreach (var location in ViewBag.Locations)
                                                        {
                                                            <option value="@location.Id">@location.Region</option>
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-calendar-alt"></span><span class="ms-1">Alış Tarihi</span>
                                                    </div>
                                                    <input type="date" asp-for="PickupDate" class="form-control">
                                                    <span asp-validation-for="PickupDate" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <script>
                                                document.getElementById('PickupDate').value = '';
                                                var today = new Date().toISOString().split('T')[0];
                                                document.getElementById('PickupDate').setAttribute('min', today);
                                            </script>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-calendar-alt"></span><span class="ms-1">İade Tarihi</span>
                                                    </div>
                                                    <input type="date" asp-for="ReturnDate" class="form-control" />
                                                    <span asp-validation-for="ReturnDate" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <script>
                                                document.getElementById('ReturnDate').value = '';
                                                var today = new Date().toISOString().split('T')[0];
                                                document.getElementById('ReturnDate').setAttribute('min', today);
                                            </script>
                                            <div class="col-12">
                                                <button type="submit" class="btn btn-light w-100 py-2">Kirala</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-lg-6 d-none d-lg-flex fadeınright animated" data-animation="fadeınright" data-delay="1s" style="animation-delay: 1s;">
                                <div class="text-start">
                                    <h1 class="display-5 text-white">Kaliteli araç filomuz ile seyahat etmenin tadını çıkartın</h1>
                                    <p>Keyifli yolculuklar</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img src="~/themes/img/carousel-1.jpg" class="img-fluid w-100" alt="first slide" />
                <div class="carousel-caption">
                    <div class="container py-4">
                        <div class="row g-5">
                            <div class="col-lg-6 fadeınleft animated" data-animation="fadeınleft" data-delay="1s" style="animation-delay: 1s;">
                                <div class="bg-secondary rounded p-5">
                                    <h4 class="text-white mb-4">Araç Kirala</h4>
                                    <form asp-controller="Home" asp-action="Filter" method="post">
                                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                        <div class="row g-3">
                                            <div class="col-12">
                                                <select asp-for="VehicleTypeId" class="form-select" aria-label="default select example">
                                                    <option selected disabled hidden>Araç modelini seç</option>
                                                    @foreach (var item in ViewBag.Models)
                                                    {
                                                        <option value="@item.Id">@item.Name</option>
                                                    }
                                                </select>
                                                <span asp-validation-for="VehicleTypeId" class="text-danger"></span>
                                            </div>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-map-marker-alt"></span> <span class="ms-1">Alış Yeri</span>
                                                    </div>
                                                    <select asp-for="PickupLocationId" class="form-select">
                                                        <option selected disabled hidden>Lokasyon seçin</option>
                                                        @foreach (var location in ViewBag.Locations)
                                                        {
                                                            <option value="@location.Id">@location.Region</option>
                                                        }
                                                    </select>
                                                    <span asp-validation-for="PickupLocationId" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <a href="#" class="text-start text-white d-block mb-2">Farklı bir iade yerine mi ihtiyacınız var?</a>
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-map-marker-alt"></span><span class="ms-1">İade Yeri</span>
                                                    </div>
                                                    <select asp-for="ReturnLocationId" class="form-select">
                                                        <option selected disabled hidden>Lokasyon seçin</option>
                                                        @foreach (var location in ViewBag.Locations)
                                                        {
                                                            <option value="@location.Id">@location.Region</option>
                                                        }
                                                    </select>
                                                    <span asp-validation-for="ReturnLocationId" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-calendar-alt"></span><span class="ms-1">Alış Tarihi</span>
                                                    </div>
                                                    <input type="date" asp-for="PickupDate" class="form-control">
                                                    <span asp-validation-for="PickupDate" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="input-group">
                                                    <div class="d-flex align-items-center bg-light text-body rounded-start p-2">
                                                        <span class="fas fa-calendar-alt"></span><span class="ms-1">İade Tarihi</span>
                                                    </div>
                                                    <input type="date" asp-for="ReturnDate" class="form-control" />
                                                    <span asp-validation-for="ReturnDate" class="text-danger"></span>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <button type="submit" class="btn btn-light w-100 py-2">Kirala</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-lg-6 d-none d-lg-flex fadeınright animated" data-animation="fadeınright" data-delay="1s" style="animation-delay: 1s;">
                                <div class="text-start">
                                    <h1 class="display-5 text-white">Kiralamalarınızda %15 indirim kazanın! modelinizi seçin </h1>
                                    <p>güvenli yolculuklar</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- carousel end -->
